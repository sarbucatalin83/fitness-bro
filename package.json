{
  "name": "fitness-bro",
  "version": "1.0.0",
  "private": true,
  "description": "FitnessBro - PWA fitness tracking app with Supabase backend",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "frontend": "npm run start --workspace=frontend",
    "frontend:build": "npm run build --workspace=frontend",
    "backend:start": "cd backend && supabase start",
    "backend:stop": "cd backend && supabase stop",
    "backend:db:push": "cd backend && supabase db push",
    "backend:functions": "cd backend && supabase functions serve",
    "backend:deploy": "cd backend && supabase functions deploy"
  },
  "keywords": ["fitness", "pwa", "angular", "supabase"],
  "author": "",
  "license": "MIT"
}
